{
  "name": "shellington",
  "version": "1.0.0",
  "description": "AI-powered shell with remote UI",
  "main": "core/index.js",
  "type": "module",
  "scripts": {
    "dev:core": "tsx watch core/index.ts",
    "dev:ui": "cd ui && npm run dev",
    "build:core": "tsc -p core/tsconfig.json",
    "build:ui": "cd ui && npm run build",
    "start:core": "node dist/core/index.js",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "keywords": ["shell", "ai", "terminal", "remote-ui"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "sqlite3": "^5.1.0",
    "blessed": "^0.1.81",
    "chalk": "^5.0.0",
    "commander": "^11.0.0",
    "express": "^4.18.0",
    "jsonwebtoken": "^9.0.0",
    "node-pty": "^1.0.0",
    "socket.io": "^4.0.0",
    "winston": "^3.0.0"
  }
}
